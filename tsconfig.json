{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "lib/store.js", "lib/hooks.js", "lib/createAppSlice.js", "lib/features/mentor/mentorSlice.js", "lib/features/auth/authAPI.js", "lib/features/auth/authSlice.js", "components/LoginCard.jsx", "components/NavCard.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
